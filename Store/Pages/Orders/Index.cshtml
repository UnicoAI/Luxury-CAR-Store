@page
@model Store.Pages.Orders.IndexModel
@{
}

<h1>Orders</h1>

<table class="table">
    <thead>
        <tr>
            <th>Order Date</th>
            <th>Product Name</th>
            <th>Quantity</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model.Orders)
        {
            <tr>
                <td>@order.OrderDate</td>
                <td>@order.Product</td>
                <td>@order.Quantity</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
